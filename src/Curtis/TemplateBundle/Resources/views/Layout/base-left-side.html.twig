<!DOCTYPE html>
<html lang="sk">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport"
          content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">

    <title>Curtis</title>

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}?v1"/>

    <meta name="description" content="">

    <!-- CSS -->
    <link href="{{ asset('bundles/curtistemplate/css/preload.css') }}" rel="stylesheet">


    <link href="{{ asset('bundles/curtistemplate/css/vendors.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/curtistemplate/css/syntaxhighlighter/shCore.css') }}" rel="stylesheet">


    <link href="{{ asset('bundles/curtistemplate/css/style-orange2.css') }}" rel="stylesheet" media="screen"
          title="default">
    <link href="{{ asset('bundles/curtistemplate/css/width-full.css') }}" rel="stylesheet" title="default">

    <link href="{{ asset('bundles/curtisapp/css/own.css') }}" rel="stylesheet">

    {% block stylesheet %}{% endblock %}

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="{{ asset('bundles/curtistemplate/js/html5shiv.min.js') }}"></script>
    <script src="{{ asset('bundles/curtistemplate/js/respond.min.js') }}"></script>
    <![endif]-->
</head>

<!-- Preloader -->
<div id="preloader">
    <div id="status">&nbsp;</div>
</div>

<body>


<div class="sb-site-container">
    <div class="boxed">

        {% include '@CurtisTemplate/Layout/navigation.html.twig' %}

        {% block breadcrumbs %}{% endblock %}

        <div class="container">
            <div class="row">
                <div class="col-md-8 col-md-push-4">
                    {% block body %}{% endblock %}
                </div>
                <div class="col-md-4 col-md-pull-8">
                    <aside class="sidebar">
                        {% block sidebar %}{% endblock %}
                    </aside> <!-- Sidebar -->
                </div>

            </div> <!-- row -->
        </div> <!-- container  -->

        {#{% include '@CurtisTemplate/Layout/footer.html.twig' %}#}

    </div> <!-- boxed -->
</div> <!-- sb-site -->
<div id="back-top">
    <a href="#header"><i class="fa fa-chevron-up"></i></a>
</div>

{% block javascript %}{% endblock %}
<script src="{{ asset('bundles/curtistemplate/js/vendors.js') }}"></script>

<!-- Syntaxhighlighter -->
<script src="{{ asset('bundles/curtistemplate/js/syntaxhighlighter/shCore.js') }}"></script>
<script src="{{ asset('bundles/curtistemplate/js/syntaxhighlighter/shBrushXml.js') }}"></script>
<script src="{{ asset('bundles/curtistemplate/js/syntaxhighlighter/shBrushJScript.js') }}"></script>

<script src="{{ asset('bundles/curtistemplate/js/DropdownHover.js') }}"></script>
<script src="{{ asset('bundles/curtistemplate/js/app.js') }}"></script>
<script src="{{ asset('bundles/curtistemplate/js/holder.js') }}"></script>
<script src="{{ asset('bundles/curtistemplate/js/home_full.js') }}"></script>

</body>

</html>