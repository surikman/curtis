<!DOCTYPE html>
<html lang="en">
<head>
    {% include '@CurtisTemplate/Layout/styles.html.twig' %}
    {% block stylesheet %}{% endblock %}
</head>

<!-- Preloader -->
<div id="preloader">
    <div id="status">&nbsp;</div>
</div>

<body>


<div class="sb-site-container">
    <div class="boxed">

        {% include '@CurtisTemplate/Layout/navigation.html.twig' %}

        {% block breadcrumbs %}{% endblock %}

        <div class="container">
            <div class="row">
                <div class="col-md-8">
                    {% block body %}{% endblock %}
                </div>
                <div class="col-md-4">
                    <aside class="sidebar">
                        {% block sidebar %}{% endblock %}
                    </aside> <!-- Sidebar -->
                </div>

            </div> <!-- row -->
        </div> <!-- container  -->

        {#{% include '@CurtisTemplate/Layout/footer.html.twig' %}#}

    </div> <!-- boxed -->
</div> <!-- sb-site -->
<div id="back-top">
    <a href="#header"><i class="fa fa-chevron-up"></i></a>
</div>

{% block javascript %}{% endblock %}
{% include '@CurtisTemplate/Layout/javascript.html.twig' %}
</body>

</html>